@model List<BookLibarySystem.Models.Order>
<!-- Start breadcrumb area -->
<div class="ht__breadcrumb__area bg-image--3" style="background-image: url('@Url.Content("~/Content/images/bg/3.jpg")');">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcrumb__inner text-center">
                    <h2 class="breadcrumb-title">Order History</h2>
                    <nav class="breadcrumb-content">
                        <a class="breadcrumb_item" href="index.html">Home</a>
                        <span class="brd-separator">/</span>
                        <span class="breadcrumb_item active">Order History</span>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End breadcrumb area -->
<!-- cart-main-area start -->
<div class="cart-main-area section-padding--lg bg--white">
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-sm-12 ol-lg-12">
                    <div class="table-content wnro__table table-responsive">
                        <table>
                            <thead>
                                <tr class="title-top">
                                    <th>Id</th>
                                    <th>Address</th>
                                    <th>Status</th>
                                    <th>View</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    var orderInfo = item.OrderInfo;
                                    <tr>
                                        <td ><a href="#">@item.OrderId</a></td>
                                        <td><span class="amount">@item.Address</span></td>
                                        <td >@item.Status</td>
                                        <td> @Html.ActionLink("View", "Index","OrderInfoes",new { orderId=item.OrderId }, null)</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
            </div>
        </div>
    </div>
</div>
<!-- cart-main-area end -->